{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
      "semicolon": [
        true,
        "never"
      ],
      "quotemark": [
        true,
        "single"
      ],
      "no-console": [false],
      "interface-name": [false],
      "trailing-comma": [false],
      "no-consecutive-blank-lines": [false]
    }
}
